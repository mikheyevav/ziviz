{
  "name": "ziviz",
  "version": "0.1.3",
  "description": "Ziviz",
  "author": "mikheyevav",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikheyevav/ziviz.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipywidgets",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/**/*.js",
    "lib/**/*.json",
    "dist/*.js"
  ],
  "scripts": {
